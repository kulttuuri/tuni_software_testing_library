{
  "scripts": {
    "istanbul": "NODE_ENV=test istanbul cover mocha test.js",
    "c8": "NODE_ENV=test c8 mocha -R mocha-lcov-reporter test.js",
    "c8coveralls": "NODE_ENV=test c8 mocha test.js --report lcovonly",
    "nyccoveralls": "nyc report --reporter=text-lcov | coveralls",
    "mochablanket": "mocha --require blanket -R html-cov > coverage.html",
    "awesome": "mocha test.js --reporter mochawesome",
    "test": "nodeunit test",
    "coveralls": "jscoverage lib && YOURPACKAGE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls"
  },
  "type": "module",
  "exports": "./test.js",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "blanket": "^1.2.3",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "grunt": "^1.5.3",
    "grunt-coveralls": "^2.0.0",
    "istanbul": "^0.4.5",
    "jscoverage": "^0.6.0",
    "mocha": "^10.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mochawesome": "^7.1.3",
    "nodeunit": "^0.11.3",
    "nyc": "^15.1.0"
  }
}
